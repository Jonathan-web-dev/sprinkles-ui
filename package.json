{
  "name": "sprinkles-ui",
  "version": "0.0.1",
  "description": "A portable, customizable set of UI components",
  "main": "index.js",
  "scripts": {
    "start": "node ./node_modules/ui-harness/server --entry ./specs",
    "test": "jest"
  },
  "keywords": [],
  "author": "Harrison Harnisch <hharnisch@signal.co>",
  "license": "MIT",
  "dependencies": {
    "react": "^0.14.6",
    "react-dom": "^0.14.6"
  },
  "devDependencies": {
    "ui-harness": "^2.0.2",
    "lorem-ipsum": "^1.0.3",
    "babel-jest": "^6.0.1",
    "jest-cli": "^0.8.2",
    "react-addons-test-utils": "^0.14.6"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "testFileExtensions": ["es6", "js", "jsx"],
    "moduleFileExtensions": ["js", "json", "es6", "jsx"]
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab01dv2.thebrighttag.com:dev/sprinkles-ui.git"
  },
  "engines": {
    "node": ">=5.0.0"
  }
}
